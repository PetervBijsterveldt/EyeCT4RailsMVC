@model List<EyeCT4RailzMVC.Models.Spoor>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Index</h2>

@Html.BeginForm()
{
@for (int i = 0; i < Model.Count; i++)
{
    <div class="spoordiv">
        @for (int j = 0; j < Model[i].Sectoren.Count; j++)
        {
            <div class="sectordiv"></div>
        } 
    </div>
}
}
